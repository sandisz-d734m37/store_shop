<%= form_with url: '/stores', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br/><br/>
  <%= form.label :description %>
  <%= form.text_field :description %><br/><br/>
  <%= form.label :address %>
  <%= form.text_field :address %><br/><br/>
  <%= form.label :sale %>
  <%= form.check_box :sale %><br/><br/>
  <%= form.label :rating %>
  <%= form.number_field :rating, in: 1..5 %><br/><br/>


  <%= form.submit "Create Store" %>
<% end %>
